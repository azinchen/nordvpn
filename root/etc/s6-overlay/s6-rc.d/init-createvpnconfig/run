#!/command/with-contenv sh
# shellcheck shell=sh

set -eu

echo "[INIT-CREATEVPNCONFIG] Starting init-createvpnconfig script..."
echo "[INIT-CREATEVPNCONFIG] Calling vpn-config to generate VPN configuration..."

vpn-config

echo "[INIT-CREATEVPNCONFIG] init-createvpnconfig script completed successfully"
exit 0
